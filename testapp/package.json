		"express": "5.1.0",
		"mustache-express": "1.3.2",
		"socket.io": "4.8.1",
		"socket.io-client": "4.8.1"
	},
	"devDependencies": {
		"@types/express": "5.0.1",
		"@types/mustache-express": "1.2.5",
		"@types/node": "22.15.17",
		"@types/socket.io": "3.0.1",
		"@types/socket.io-client": "1.4.36",
		"concurrently": "9.1.2",
		"ts-node-dev": "2.0.0",
		"typescript": "5.9.2"
	}
